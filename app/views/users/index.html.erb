<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Role</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.role %></td>        
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to('New User', new_user_path) %>